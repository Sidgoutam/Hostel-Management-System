   <children>
      <ComboBox fx:id="Hostelno" layoutX="347.0" layoutY="153.0" onMouseClicked="#loadhostel" prefWidth="150.0" promptText="Select" />
      <Label layoutX="99.0" layoutY="151.0" text="Choose Hostel:" textFill="#6f1b1b">
         <font>
            <Font size="25.0" />
         </font></Label>
      <Button layoutX="456.0" layoutY="247.0" mnemonicParsing="false" onAction="#deletehostel" text="Delete" />
      <Button layoutX="15.0" layoutY="1.0" mnemonicParsing="false" onAction="#rooms" text="Back" />
   </children>
</AnchorPane>
